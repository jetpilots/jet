decl func matches(s String, re Regex) Boolean

test "match string"
  var d = 0
  match "uioyt"
  case "poi"
    d = 9
  case "ftyyrd", "uioyt", "nimju"
    d = 8
  end
  check d ==8

  match "ident787687"
  case `[a-z]+`
    d = 77
  case `[0-9]+`
    d = 66
  end
  check d == 77

  var reIdent = `^[A-Z][a-z0-9_]*$`
  check matches("Type", re=reIdent)

end

type Obj
  var c = 0
end

func t1()
  ans = 7
end

func t4()
  ans = Obj()
  ans.c = 9
end

func t5()
  ans = t4()
  ans = Obj()
end

test "ans"
  var a = t4()
  check a.c == 9
  a = t5()
  check a.c == 0
  var f = t1()
  check f == 7
end

test "dicts"
  var d = {
    "n" = 6,
    "k" = 8}
  check d["n"] == 6
  check d["i"] == 0
end

test "arrays"
  var d = [4, 3, 2, 1, 5]
